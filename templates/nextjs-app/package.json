{
  "$schema": "https://json.schemastore.org/package",
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev": "turbo run dev:next",
    "build:next": "next build",
    "build": "turbo run build:next",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.7",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.8",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.1",
    "tailwind-merge": "^1.13.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "10.4.14",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "shell-quote": "^1.8.1",
    "tailwindcss": "3.3.2",
    "turbo": "latest",
    "typescript": "5.1.3",
    "vitest": "^0.32.2"
  }
}
