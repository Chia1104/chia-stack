{
  "$schema": "https://json.schemastore.org/package",
  "name": "@chia-stack/react-ui",
  "version": "0.0.0",
  "private": true,
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup && tailwindcss -i ./src/style/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup --watch\" \"tailwindcss -i ./src/style/styles.css -o ./dist/styles.css --watch\"",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.{ts,js,tsx,jsx}\" --fix",
    "type:check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "devDependencies": {
    "@chia-stack/tailwind-config": "workspace:*",
    "@chia-stack/usehook": "workspace:*",
    "@radix-ui/react-accessible-icon": "^1.0.2",
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-alert-dialog": "^1.0.3",
    "@radix-ui/react-aspect-ratio": "^1.0.2",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-checkbox": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.2",
    "@radix-ui/react-context-menu": "^2.1.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-hover-card": "^1.0.5",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-menubar": "^1.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.2",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-progress": "^1.0.2",
    "@radix-ui/react-radio-group": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.0.3",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-separator": "^1.0.2",
    "@radix-ui/react-slider": "^1.1.1",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-switch": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.5",
    "@types/react": "18.0.27",
    "@types/react-dom": "^18.0.11",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "framer-motion": "^10.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animate": "^1.0.5",
    "tsconfig": "workspace:0.0.0",
    "tsup": "^6.7.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "engines": {
    "node": ">=16"
  }
}
