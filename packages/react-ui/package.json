    "tailwind-variants": "^0.1.5",
    "tailwindcss": "^3.3.1",
    "tsconfig": "workspace:0.0.0",
    "tsup": "^6.7.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    "./package.json": "./package.json",
    "./src/style/styles.css": "./dist/index.css",
    ".": {
